<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="MovieRequestMapper">
	<!-- insert 되어 있다면, 신청 중인 상태 > List -->
	<select id="movieRequestList" parameterType="java.util.Map" resultType="com.moco.movieRequest.MovieRequestDTO">
		select * from
		(select rownum R, M.* from
		(select * from movieRequest order by num desc) M)
		where R between #{row.startRow} and #{row.lastRow} 
	</select>
</mapper>